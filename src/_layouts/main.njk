---
fluid: false
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>EAMT2024</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/katex@0.16.10/dist/katex.min.css" integrity="sha384-wcIxkf4k558AjM3Yz3BBFQUbk/zgIYC2R0QpeeYb+TwlBVMrlgLqwRjRtGZiK7ww" crossorigin="anonymous">
    <link rel="stylesheet" href="/assets/custom.css">

    <script src="https://kit.fontawesome.com/b7b8224508.js" crossorigin="anonymous"></script>
    <link href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css' rel='stylesheet'>

  </head>
  <body>
    <header class="gsites_header">
      <div class="header_img_contain">
        <div class="header_img">
        </div>
        <h1 class="page_title">{{ title }}</h1>
      </div>
    </header>
    {# {% include "nav.njk" %} #}
    <main class="{% if fluid %}container-fluid{%else%}container{%endif%} pb-3">
      {{ content | safe }}
    </main>

    <footer class="text-body-secondary py-5">
      <div class="container">
        <p>For corrections, please <a href="https://github.com/EAMT2024/eamt2024.github.io/issues" target="_blank">open a GitHub issue</a> or contact <a href="mailto:eamt2024@gmail.com">the organisers</a>.</p>
      </div>
    </footer>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/katex@0.16.10/dist/katex.min.js" integrity="sha384-hIoBPJpTUs74ddyc4bFZSM1TVlQDA60VBbJS0oA934VSz82sBx1X7kSx2ATBDIyd" crossorigin="anonymous"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/katex@0.16.10/dist/contrib/auto-render.min.js" integrity="sha384-43gviWU0YVjaDtb/GhzOouOXtZMP/7XUzwPTstBeZFe/+rCMvRwr4yROQP43s0Xk" crossorigin="anonymous"></script>

    <script defer src="/assets/math.js"></script>
    <script defer src="/assets/collapse.js"></script>
    <script defer src="/assets/session-badge.js"></script>

    {% for script in scripts %}
      <script defer src="{{ script }}"></script>
    {% endfor %}
  </body>
</html>